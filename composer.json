{
  "name": "surgio/laroute",
  "description": "Access Laravels URL/Route helper functions, from JavaScript.",
  "keywords": [
    "laravel",
    "routes",
    "routing",
    "javascript"
  ],
  "authors": [
    {
      "name": "Aaron Lord",
      "email": "hello@aaronlord.is"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|~6",
    "illuminate/routing": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|~6",
    "illuminate/console": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|~6",
    "illuminate/config": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|~6",
    "illuminate/filesystem": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|~6"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Lord\\Laroute\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
        "providers": [
            "Lord\\Laroute\\LarouteServiceProvider"
        ]
    }
}
}
