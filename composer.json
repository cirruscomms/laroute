{
  "name": "swoop/laroute",
  "description": "Access Laravels URL/Route helper functions, from JavaScript.",
  "keywords": [
    "laravel",
    "routes",
    "routing",
    "javascript"
  ],
  "authors": [
    {
      "name": "Steven White",
      "email": "steven.white@swoop.com.au"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3.0",
    "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
    "illuminate/routing": "^5.0|^6.0|^7.0|^8.0",
    "illuminate/console": "^5.0|^6.0|^7.0|^8.0",
    "illuminate/config": "^5.0|^6.0|^7.0|^8.0",
    "illuminate/filesystem": "^5.0|^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^5.0",
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Swoop\\Laroute\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
        "providers": [
            "Swoop\\Laroute\\LarouteServiceProvider"
        ]
    }
}
}
